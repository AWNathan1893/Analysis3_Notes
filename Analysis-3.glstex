\bibglsnewsymbol{ball}%
{sort={ball|}}%
{\ensuremath{B_r(a)}}%
{Open ball centered at \ensuremath{a \in \mathbb{R}^n} of radius \ensuremath{r}}


\bibglsnewsymbol{boundary}%
{sort={boundary|}}%
{\ensuremath{\partial S}}%
{Boundary of a set \ensuremath{S \subseteq \mathbb{R}^n}}


\bibglsnewsymbol{boundS}%
{sort={boundS|}}%
{\ensuremath{\mathscr{B}(S)}}%
{Space of all bounded functions defined over \ensuremath{S}}


